        <div class="container" style="max-width: 900px;">
            <div class="row" style="margin-bottom: 3em">
                <div class="col-sm-6">
                    <a style="display: block; margin-bottom: 1em">Shipping Info</a>
                    <a style="display: block; margin-bottom: 1em">Contact Us</a>
                    <a style="display: block; margin-bottom: 1em">My Account</a>
                </div>
                <div class="col-sm-6"></div>
            </div>
            <div class="row copyright" style="margin-bottom: 3em; text-align: center">
                <div class="col-sm-12">&copy; <%= new Date().getFullYear() %> COCOHONEY COSMETICS</div>
            </div>
        </div>
    </footer>
    <script>
        $("#banner-1").children("div:gt(0)").css({ left: "100%" });
        (function banner_autoplay() {
            setTimeout(function() {
                $("#banner-1 .slide").eq(0).animate({ left: "-100%" }, 800, function() {
                    $(this).next(".slide").animate({ left: "0" }, 800, function() {
                        $(this).prev().css({ left: "100%" }).appendTo("#banner-1");
                        banner_autoplay();
                    });
                });
            }, 5000)
        })();
    </script>
