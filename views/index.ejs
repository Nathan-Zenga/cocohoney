        <script>
            $(".bg-reel").each(function() { // bg auto-playing slideshow
                var bgreel = this;
                $(bgreel).children("div:gt(0)").fadeTo(0, 0);
                setInterval(function() {
                    $(bgreel).children('div:first')
                        .fadeTo(2000, 0)
                        .dequeue()
                        .next()
                        .fadeTo(0, 1)
                        .end()
                        .appendTo(bgreel);
                }, 5000);
            });

            $(window).on("load", function() {
                $("#loadscreen").fadeOut(1000, function() { $(this).remove() })
            });
        </script>
<%_ include partials/footer _%>
